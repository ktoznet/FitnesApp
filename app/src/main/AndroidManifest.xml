<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Fitness">

    <meta-data
        android:name="com.google.android.gms.version"
        android:value="@integer/google_play_services_version" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/fitness"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Fitness">
        <activity
            android:name=".presentor.weight_loss.WeightLossActivity"
            android:exported="false" />
        <activity
            android:name=".presentor.pressActivity.PressActivity"
            android:exported="false" />
        <activity
            android:name=".presentor.firstActivity.ChoiceActivity"
            android:exported="true"
            android:theme="@style/Theme.App.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".presentor.strongActivty.MainActivity"
            android:exported="false" />
    </application>

</manifest>